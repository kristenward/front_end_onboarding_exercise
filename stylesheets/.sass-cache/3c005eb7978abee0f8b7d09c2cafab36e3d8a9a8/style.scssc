:@template"©#/* widths */
$navwidth: 230px;
$navtitle: 80px;

/* colors */
$styled-border1: url('../../images/ui/cat-nav-border-image.png');
$styled-border2: url('../../images/ui/aside-news-bg.png');
$sprites: url('../../images/ui/sprite-sheet.png');
$fpp-darkgrey: #3e3e3e;
$fpp-medgrey: #7e7e7e;
$fpp-magenta: #ff008a;
$fpp-background: #ffffff;

/* fonts */

@mixin scriptfont {
    font-family: 'Georgia', serif;
    font-style:italic;
}

@mixin sansreg{
    font-family: 'Lato', sans-serif;
    font-weight:normal;
}

@mixin sansthin {
    font-family: 'Lato', sans-serif;
    font-weight:300;
}

/* cross-browser */

@mixin sb1($th){ /*thickness argument*/

    border: $th+px solid transparent;
    -webkit-border-image:$styled-border1 $th $th round; /* Safari 5 */
    -o-border-image:$styled-border1 $th $th round; /* Opera */
    border-image:$styled-border1 $th $th round;
}
@mixin sb2($th){ /*thickness argument*/

    border: $th+px solid transparent;
    -webkit-border-image:$styled-border2 $th $th round; /* Safari 5 */
    -o-border-image:$styled-border2 $th $th round; /* Opera */
    border-image:$styled-border2 $th $th round;
}

/* nav / asides */

a{
    text-decoration:none;
}
h2.aside-h{
    @include scriptfont;
    font-size:12px;
    text-transform:uppercase;
    font-weight:100;
    letter-spacing:1px;
}
.wl-navborder{
    position:relative;
    margin:10px 0;
    @include sb1(1);
    
}
.nav-title{
    position: absolute;
    z-index:100;
    top: -10px;
    width: $navwidth;
    @include scriptfont;
    font-size:14px;
    text-align: center;
    span{
        z-index:5px;
        padding:0 5px;
        background:$fpp-background;
    }
}
ul.wl-ui-list{
    margin: 0;
    padding:0;
    @include sansreg;
    font-size: 10px;
    text-transform: uppercase;
    list-style-type: none; 
    .arrow{
        display:inline-block; 
        position:relative;
        top: -2px;
        left:5px;
        width:7px; height:4px;
        background-image: $sprites;
        background-position:7px 7px;
    }
    .active-cat>a>.arrow{
        background-position: -7px 0px;
    }
    .active-parent>a>.arrow{
        background-position: 0px 0px;
    }
    a{
        display:block;
        padding:8px 20px;
        line-height:16px;
        color:#7f7f7f;
    }
    a:hover{
        color:$fpp-darkgrey;
        background:#f7f7f7;
    }
    
    li.active-cat>a{
        color:#eb5190;
    }
    li.active-parent>a{
        color:$fpp-darkgrey;
    }
    ul.wl-sub{
        a{
            padding-left:30px;
        }
        .wl-sub2{
            a{
                padding-left:40px;
            }
        }
    }
}
ul.wl-nav{
    padding:10px 0 0 0;
}

/* newsletter */
.wl-asidenews:before{
    content: '';
    display:block;
    position:absolute;
    top:100px;
    left:-19px;
    width:52px;
    height:31px;
    background:$sprites;
    background-position:0px -4px;
}
.wl-asidenews{
    position:relative;
    margin:10px 0;
    padding:18px;
    text-align:center;
    @include sb2(2);
    h2.aside-h{
         margin:0;
         color: $fpp-magenta;
    }
    p{
        margin:0; 
        @include sansreg;
        text-transform:lowercase;
        color:$fpp-darkgrey;
        font-size:11px; line-height:20px;
        font-weight:300;
        letter-spacing:1px;
    }
    input{
        display:inline-block;
        margin:10px 0 0 0;
        clear:both;
    }
    input[type=text]{
        padding:7px 10px;
        width:170px;
        @include scriptfont;
        color:#bfbfbf;
        font-size:12px; 
    }
    input[type=submit]{
        padding:8px 18px;
        color:white;
        @include sansreg;
        font-weight:500;
        font-size:11px;
        background: $fpp-magenta;
        @include sb2(1);
        cursor:pointer;
    }
}

/* Swipe 2 required styles */
.wl-swipe{
    padding-top:7px;
    margin-bottom:20px;
    width:230px;
    nav{
        position: absolute;
        bottom:-16px;
        width:100%;
    }
}
.swipe {
    position: relative;
    overflow: hidden;
    visibility: hidden;
  
}
.swipe-wrap {
    overflow: hidden;
    position: relative;
}
.swipe-wrap > div {
    position: relative;
    width:100%;
    height:260px;
    float:left;
}
.wl-swipe swipe div b{
    display:block;
    margin:10px;
    padding:100px 10px;
    height:260px;
    font-weight:bold;
    color:#14ADE5;
    font-size:20px;
    text-align:center;
    box-shadow: 0 1px #EBEBEB;
    background: #fff;
    border-radius: 3px;
    border: 1px solid;
    border-color: #E5E5E5 #D3D3D3 #B9C1C6;
}
.swipe-position li {
    display: inline-block; 
    margin: 0 2px;
    width: 6px; height: 6px; 
    background: #ccc;
    border-radius: 6px;
    cursor: pointer;
}
.swipe-position li.on {
    background-color: #ff4c96;
}
ul.swipe-position{
    margin:0 auto; padding:0; 
    text-align:center;
}


/* / / Product Grid / / */
.wl-product-group--grid {
    margin: 0;
    padding:0;
    list-style: none;
    .wl-product {
        position: relative;
        margin: 0 0 5px 0;
        padding: 0 0;
        width: 241px;
        min-height: 400px;
        _height: 400px;
        display:block;
        float: left;
        .media {
            margin: 0 0 6px;
            width: 236px;
            height: 314px;
            overflow: hidden;
            a {
                display: block;
                img {
                    display: block;
                }
            }
        }

        .info{
            padding: 0 20px 0 0;
            .wl-swatches {
                margin: 0 0 6px;
            }
            h2.name {
                margin: 0 0 1px;
                font-size: 13px;
                font-weight:normal;
                letter-spacing:1px;
      
                a{
                    font-family: 'Georgia', serif;
                    color:$fpp-darkgrey;
                }
            }
            .description {
                margin: 0;
                line-height: 1.5;

            }
            .prices {
                margin: 0 0 4px;
                font-size: 14px;
                text-transform: uppercase;
                color:$fpp-medgrey;
                @include sansthin;
                .price {
                    display: inline;
                    margin: 0;
                }
                .price--original {
                }
                .price--sale {
                      color: #C54551;
                }
            }
        }   
        .wl-action {
            $width: 236px;
            display: none;
            position: absolute;
            left: 0;
            top: 0;
            margin: 0;
            height:314px;
            width: $width;
            background:rgba(255,255,255,.15);
            .wl-button {
                position:absolute;
                bottom:4px;
                right:4px;
                width: 30px;
                height:30px;
                overflow:hidden;
                text-indent:-9999px;
                background-image: $sprites;
                background-position:0px -35px;
            }
            .wl-video{
                bottom:auto; right:auto;
                top:10px; left:10px;
                width:25px;
                height:25px;
                background-position:-60px -35px;
            }
            .wl-button:hover{
                background-position:-30px -35px;
            }
            .wl-video:hover{
                background-position:-85px -35px;
            }
        }
        &:hover {
            .wl-action {
                display: block;
            }
        }
    }
}

/* Color swatches */
.wl-product-swatches{
    display:inline;    
    width:20px; padding:0;
    list-style-type:none;
    li{
        display:inline-block;
        margin:0; padding:0;
        cursor:pointer;
    }
     div.addl{
        display:inline-block;
        position:relative; top:-5px; 
        color:#eb5190;
        @include sansreg;
        font-weight:bold;
        font-size:10px;
    }
}

/* Refinements */
.wl-refinements{
    position:relative;
    width:100%;
    height:40px;
    background: $sprites;
    background-position:0 -89px;
    background-repeat:repeat-x;
    form{
        position:absolute;
        top:0;
        left:0;
        padding:0 5px;
        background:$fpp-background;
        select{
            -moz-appearance:none;
            -webkit-appearance: none;
            padding:5px;
            width:150px;
            background: transparent $sprites;
            background-position: 0px -65px;
            @include scriptfont;
            color:$fpp-medgrey;
        }
    }
    .refine-results{
        position:absolute;
        bottom:0;
        right:0;
        padding:0 10px 0 5px;
        height:40px; 
        line-height:40px;
        @include scriptfont;
        color:$fpp-medgrey;
        font-size:12px;
        background:$fpp-background;
    }
}
/* Layout Containers */

.sidebar{
    padding:0;
    margin:10px;
    width:$navwidth;
    float:left;
}
.product-container{
    margin:0;
    padding-left:250px;
    min-width:730px;
}
